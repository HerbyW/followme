<path>followme2.ac</path>


<nasal>
  <load>
    var livery_update = aircraft.livery_update.new("Aircraft/followme/Models/Liveries", 10);
    var update = aircraft.overlay_update.new();
    update.add("Aircraft/followme/Models/Sign", "sim/multiplay/generic/string[2]");
  </load>   
  <unload>
    livery_update.stop();
    update.stop();
  </unload>   
</nasal>

<sound>
  <path>Aircraft/followme/Sounds/followme-sound.xml</path>
</sound> 
  
<animation>
 <type>material</type>
 <object-name>sign</object-name>
 <property-base>sim/model/message</property-base>
 <texture-prop>texture-message</texture-prop>
 <texture>follow_me.png</texture>
 <emission>
    <red>   1 </red>
    <green> 1 </green>
    <blue>  1 </blue>
  </emission>
</animation>

<animation>
  <type>material</type>
  <object-name>ldoor_1</object-name>
  <object-name>Mesh11_2</object-name>  
  <object-name>door.rl_1</object-name>
  <object-name>door.rr_1</object-name>
  <object-name>Mesh76.004_1</object-name>
  <property-base>sim/model/livery</property-base>
  <texture-prop>texture</texture-prop>
  <texture>followmeoutside.png</texture>  
  <emission>
    <factor-prop>sim/time/sun-angle-rad</factor-prop>
    <red>   0.1 </red>
    <green> 0.1 </green>
    <blue>  0.1 </blue>
  </emission>
</animation>

